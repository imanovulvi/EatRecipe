@using Domen.EatRecipe.Entitys
@model List<Page>
@{
    ViewData["Title"] = "PageList";
}

<h1>PageList</h1>
<a class="btn btn-primary" href="/admin_panel/Page/GetAdd" role="button">Link</a>
<table class="table table-striped table-hover">
 <thead>
     <tr>
         <th>Title</th>
          <th>Content</th>
            <th>CreateDate</th>
            <th>Aktiv</th>
            <th>IsDelete</th>
            <th>Update</th>
            <th>Deleted</th>
     </tr>
 </thead>
 <tbody>
     @foreach (Page item in Model)
        {
            <tr>
                <td>@item.Title</td>
                <td>@Html.Raw(item.Content)</td>
                <td> @item.CreateDate</td>
                <td>@item.Aktiv</td>
                <td>@item.IsDeleted</td>
                <td><a href="/admin_panel/Page/GetUpdate/@item.Id"><button type="button" class="btn btn-primary">Update</button></a></td>
                <td><a href="/admin_panel/Page/GetDeleted/@item.Id" onclick="return confirm('Do you deleted?')"><button type="button" class="btn btn-danger">Deleted</button></a></td>
               
                <td></td>
            </tr>
        }
 </tbody>
</table>

